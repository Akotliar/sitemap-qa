{
  "name": "@akotliar/sitemap-qa",
  "version": "1.0.0-alpha.5",
  "description": "Detect test/qa/dev/staging URLs, admin paths, sensitive parameters, and URLs that shouldn't be publicly indexed.",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "sitemap-qa": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Akotliar/sitemap-qa.git"
  },
  "bugs": {
    "url": "https://github.com/Akotliar/sitemap-qa/issues"
  },
  "homepage": "https://github.com/Akotliar/sitemap-qa#readme",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "perf-check": "tsx scripts/measure-performance.ts"
  },
  "keywords": [
    "sitemap",
    "qa",
    "testing",
    "analysis"
  ],
  "author": "Alexander Kotliar <railtokdevelopment@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "fast-xml-parser": "^5.3.3",
    "js-yaml": "^4.1.1",
    "micromatch": "^4.0.8",
    "undici": "^7.16.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/js-yaml": "^4.0.9",
    "@types/micromatch": "^4.0.10",
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
